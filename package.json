{
  "name": "star-wars-galaxy-v1",
  "private": true,
  "version": "1.0.0-final",
  "description": "Star Wars Galaxy - Complete Frontend Application - Phase 1 Complete",
  "keywords": [
    "star-wars",
    "react",
    "vite",
    "galaxy",
    "swapi",
    "production-ready"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "vite build --mode analyze",
    "clean": "rm -rf dist node_modules/.vite",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.18",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
